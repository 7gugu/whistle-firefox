{
    "manifest_version": 3,
    "name": "Whistle-Switcher",
    "description": "A Firefox Extension that can help developer switch whistle proxy easily.",
    "version": "1.0.0",
    "action": {
        "default_icon": {
            "16": "icon-16.png",
            "48": "icon-48.png",
            "128": "icon-128.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon-16.png",
        "48": "icon-48.png",
        "128": "icon-128.png"
    },
    "host_permissions": ["https://*/*"],
    "incognito": "spanning",
    "permissions": ["tabs", "activeTab", "scripting", "proxy", "storage"]
}
